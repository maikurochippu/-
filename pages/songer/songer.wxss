/* pages/calender/calender.wxss */
.navScroll {
  display: flex;
  white-space: nowrap;
  height: 60rpx;
}


.navScroll .navItem {
  padding: 0 30rpx;
  font-size: 28rpx;
  height: 60rpx;
  line-height: 60rpx;
}
.navScroll .navContent {
  height: 60rpx;
  box-sizing: border-box;
}


.navItem .active {
  border-bottom: 1rpx solid #17abe3;
}


/* 列表 */
.list{
  padding: 20rpx;
}
.listScroll{
  margin-top: 10rpx;
  height: calc(100vh - 152rpx);
  display: flex;
  flex-wrap: wrap;
}
.listScroll .scrollItem{
  width: 200rpx;
  margin: 19rpx;
}
.listScroll .scrollItem image{
  width: 200rpx;
  height: 200rpx;
  border-radius: 10rpx;
}
.listScroll .scrollItem text{
  font-size: 26rpx;
  width: 200rpx;
  white-space: nowrap; 
  display: block;
  overflow: hidden;
  text-overflow: ellipsis;
  text-align: center;
}